{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "start": "node -r esbuild-register ./src/index.ts",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "@tutly/jest-presets/node"
  },
  "dependencies": {
    "@tutly/logger": "workspace:*",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tutly/eslint-config": "workspace:*",
    "@tutly/jest-presets": "workspace:*",
    "@tutly/typescript-config": "workspace:*",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "esbuild": "^0.27.1",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "typescript": "5.9.3"
  }
}