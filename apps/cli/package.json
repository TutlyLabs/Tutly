{
  "name": "tutly",
  "description": "Tutly CLI - Command line interface for Tutly platform",
  "version": "1.1.9",
  "author": "Tutly",
  "bin": {
    "tutly": "./bin/run"
  },
  "bugs": "https://github.com/TutlyLabs/Tutly/issues",
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "@oclif/command": "^1.8.36",
    "@oclif/config": "^1.18.17",
    "@oclif/plugin-help": "^6.2.36",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "chokidar": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "fast-glob": "^3.3.3",
    "ignore": "^7.0.5",
    "keytar": "^7.9.0",
    "listr2": "^9.0.5",
    "node-pty": "^1.0.0",
    "open": "^11.0.0",
    "ora": "^9.0.0",
    "p-limit": "^7.2.0",
    "tslib": "^2.8.1",
    "update-notifier": "^7.3.1",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^4.1.15",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/chai": "^5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/mocha": "^10",
    "@types/node": "^25.0.2",
    "@types/update-notifier": "^6.0.8",
    "@types/ws": "^8.18.1",
    "chai": "^6",
    "eslint": "^9.39.2",
    "eslint-config-oclif": "^6.0",
    "eslint-config-oclif-typescript": "^3.1",
    "globby": "^16.0.0",
    "mocha": "^11.7.5",
    "nyc": "^17.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/TutlyLabs/Tutly/tree/main/apps/cli",
  "keywords": [
    "tutly",
    "cli",
    "education",
    "assignments",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tutly",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./lib/hooks/init/check-update"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TutlyLabs/Tutly.git",
    "directory": "apps/cli"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "postinstall": "npm rebuild node-pty",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && npm run build && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
