{
  "productConfiguration": {
    "nameShort": "Tutly Playgrounds",
    "nameLong": "Tutly Playgrounds - VSCode Web",
    "applicationName": "tutly-playgrounds",
    "dataFolderName": ".tutly-playgrounds",
    "version": "1.75.0",
    "quality": "stable",
    "extensionEnabledApiProposals": {
      "tutly.fsrelay": [
        "fileSearchProvider",
        "textSearchProvider"
      ]
    },
    "extensionKind": {
      "tutly.fsrelay": ["ui", "workspace"]
    }
  },
  "configurationDefaults": {
    "explorer.sortOrder": "default",
    "files.autoSave": "afterDelay",
    "explorer.compactFolders": false,
    "workbench.view.alwaysShowHeaderActions": true,
    "files.exclude": {
      "**/.vscode": true,
      "**/.git": true,
      "**/.DS_Store": true,
      "**/.data": true,
      "*.rar": true,
      "*.tar": true,
      "*.tar.gz": true,
      "*.tgz": true
    },
    "editor.autoIndent": "full",
    "editor.fontFamily": "'JetBrains Mono', 'Source Code Pro', Menlo, Monaco, 'Courier New', monospace",
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "editor.guides.indentation": false,
    "breadcrumbs.enabled": true,
    "workbench.editor.dragToOpenWindow": false,
    "workbench.editor.revealIfOpen": true,
    "workbench.tips.enabled": false,
    "workbench.editor.enablePreview": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.activityBar.visible": true
  }
}
