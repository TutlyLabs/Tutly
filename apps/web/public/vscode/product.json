{
  "productConfiguration": {
    "nameShort": "Tutly VSCode",
    "nameLong": "Tutly VSCode Web",
    "applicationName": "tutly-vscode-web",
    "dataFolderName": ".tutly-vscode-web",
    "version": "1.75.0",
    "quality": "stable",
    "commonlyUsedSettings": [
      "files.autoSave", 
      "editor.fontSize", 
      "editor.fontFamily", 
      "editor.tabSize", 
      "editor.renderWhitespace", 
      "editor.cursorStyle", 
      "editor.multiCursorModifier", 
      "editor.insertSpaces", 
      "editor.wordWrap", 
      "files.exclude", 
      "files.associations", 
      "workbench.editor.enablePreview",
      "workbench.layoutControl.type"
    ],
    "extensionEnabledApiProposals": {
      "tutly.fsrelay": [
        "fileSearchProvider",
        "textSearchProvider"
      ]
    },
    "extensionKind": {
      "tutly.fsrelay": ["ui", "workspace"]
    },
    "hiddenViewDescriptors": [
      "workbench.view.extensions",
      "workbench.view.debug",
      "workbench.view.extension.test"
    ]
  },
  "configurationDefaults": {
    "explorer.sortOrder": "default",
    "files.autoSave": "on",
    "explorer.compactFolders": false,
    "workbench.view.alwaysShowHeaderActions": true,
    "files.exclude": {
      "**/.vscode": true,
      "**/.git": true,
      "**/.DS_Store": true,
      "**/.tutly/workspace.json": true,
      "**/.data": true,
      "*.rar": true,
      "*.tar": true,
      "*.tar.gz": true,
      "*.tgz": true
    },
    "editor.autoIndent": "full",
    "editor.fontFamily": "'JetBrains Mono', 'Source Code Pro', Menlo, Monaco, 'Courier New', monospace",
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "editor.guides.indentation": false,
    "breadcrumbs.enabled": false,
    "workbench.editor.dragToOpenWindow": false,
    "workbench.editor.revealIfOpen": true,
    "workbench.tips.enabled": false,
    "workbench.editor.enablePreview": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.activityBar.visible": true
  }
}