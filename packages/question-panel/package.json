{
  "name": "question-panel",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.0.0",
  "publisher": "tutly",
  "license": "MIT",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "browser": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tutlyQuestionContainer",
          "title": "Question",
          "icon": "$(lightbulb)"
        }
      ]
    },
    "views": {
      "tutlyQuestionContainer": [
        {
          "id": "tutlyQuestionPanel",
          "name": "Question",
          "when": "true",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "tutlyQuestion.refresh",
        "title": "Refresh Question",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "tutlyQuestion.refresh",
          "when": "view == tutlyQuestionPanel",
          "group": "navigation"
        }
      ]
    }
  }
}