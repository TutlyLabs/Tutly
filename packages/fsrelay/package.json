{
  "name": "@tutly/fsrelay",
  "displayName": "Tutly File System Provider",
  "description": "A VS Code extension that provides file system and terminal access",
  "version": "0.0.1",
  "publisher": "tutly",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "browser": "./dist/extension.js",
  "contributes": {
    "themes": [
      {
        "id": "Default Dark+",
        "label": "Default Dark+",
        "uiTheme": "vs-dark",
        "path": "./themes/dark_plus.json"
      },
      {
        "id": "Default Dark Modern",
        "label": "Default Dark Modern",
        "uiTheme": "vs-dark",
        "path": "./themes/dark_modern.json"
      },
      {
        "id": "Default Light+",
        "label": "Default Light+",
        "uiTheme": "vs",
        "path": "./themes/light_plus.json"
      },
      {
        "id": "Default Light Modern",
        "label": "Default Light Modern",
        "uiTheme": "vs",
        "path": "./themes/light_modern.json"
      },
      {
        "id": "Visual Studio Dark",
        "label": "Visual Studio Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark_vs.json"
      },
      {
        "id": "Visual Studio Light",
        "label": "Visual Studio Light",
        "uiTheme": "vs",
        "path": "./themes/light_vs.json"
      }
    ],
    "commands": [
      {
        "command": "tutlyfs.run",
        "title": "Run Code"
      },
      {
        "command": "tutlyfs.submit",
        "title": "Submit Code"
      },
      {
        "command": "tutlyfs.save",
        "title": "Save Changes"
      }
    ],
    "keybindings": [
      {
        "command": "tutlyfs.run",
        "key": "ctrl+alt+shift+r",
        "mac": "cmd+alt+shift+r"
      },
      {
        "command": "tutlyfs.submit",
        "key": "ctrl+alt+shift+s",
        "mac": "cmd+alt+shift+s"
      },
      {
        "command": "tutlyfs.save",
        "key": "ctrl+alt+shift+v",
        "mac": "cmd+alt+shift+v"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tutly-container",
          "title": "Tutly",
          "icon": "$(repo)"
        }
      ]
    },
    "views": {
      "tutly-container": [
        {
          "type": "webview",
          "id": "tutly.webview",
          "name": "Tutly",
          "visibility": "visible"
        }
      ]
    },
    "configuration": {
      "title": "Tutly FS",
      "properties": {
        "tutlyfs.assignmentId": {
          "type": "string",
          "default": "",
          "description": "Assignment ID for the repository"
        },
        "tutlyfs.submissionId": {
          "type": "string",
          "default": "",
          "description": "Submission ID for the repository"
        },
        "tutlyfs.type": {
          "type": "string",
          "enum": [
            "TEMPLATE",
            "SUBMISSION"
          ],
          "default": "TEMPLATE",
          "description": "Repository type"
        },
        "tutlyfs.mode": {
          "type": "string",
          "enum": [
            "clone",
            "ondemand"
          ],
          "default": "clone",
          "description": "File system mode: 'clone' (full repo, writable ephemeral) or 'ondemand' (read-only)"
        },
        "tutlyfs.webOrigin": {
          "type": "string",
          "default": "",
          "description": "Web origin for loading webview resources"
        }
      }
    },
    "terminal": {
      "profiles": [
        {
          "id": "tutly.terminal-profile",
          "title": "Tutly Terminal",
          "icon": "$(terminal)"
        }
      ]
    }
  },
  "scripts": {
    "build": "webpack --mode production && webpack --config webpack-react.config.js",
    "build-extension": "webpack --mode production",
    "build-webview": "webpack --config webpack-react.config.js",
    "dev": "webpack --mode development --watch",
    "dev-webview": "webpack --config webpack-react.config.js --watch",
    "watch": "npm-run-all --parallel dev dev-webview",
    "package": "pnpm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.106.1",
    "autoprefixer": "^10.4.22",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.3",
    "mini-css-extract-plugin": "^2.9.4",
    "mocha": "^11.7.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.17",
    "ts-loader": "^9.5.4",
    "typescript": "5.9.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@tiptap/extension-highlight": "^3.13.0",
    "@tiptap/extension-image": "^3.13.0",
    "@tiptap/extension-list": "^3.13.0",
    "@tiptap/extension-subscript": "^3.13.0",
    "@tiptap/extension-superscript": "^3.13.0",
    "@tiptap/extension-task-item": "^3.13.0",
    "@tiptap/extension-task-list": "^3.13.0",
    "@tiptap/extension-text-align": "^3.13.0",
    "@tiptap/extension-typography": "^3.13.0",
    "@tiptap/html": "^3.13.0",
    "@tiptap/starter-kit": "^3.13.0",
    "@uiw/react-md-editor": "^4.0.11",
    "@vscode-elements/elements": "^2.3.1",
    "buffer": "^6.0.3",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "minimatch": "^10.1.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  }
}
